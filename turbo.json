{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "format": {},
    "dev": {},
    "clean:all": {},
    "ci:test": {
      "outputs": [".next/**"]
    },
    "start": {
      "dependsOn": [
        "^build",
        "ci:test"
      ],
      "env": [],
      "outputs": [".next/**", "dist/**"]
    }
  }
}

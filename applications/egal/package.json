    "@types/micro-cors": "^0.1.3",
    "@types/react": "18.2.29",
    "@types/turndown": "^5.0.3",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.4.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.11.0",
    "start-server-and-test": "^1.15.3",
    "tailwindcss": "^3.2.6",
    "ts-jest": "^29.0.5"
  }

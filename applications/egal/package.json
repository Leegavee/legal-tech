{
  "name": "@legavee/egal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:all": "rm -rf ./.next && rm -rf ./node_modules && rm -rf ./.turbo",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build_and_start": "npm run build && npm run start",
    "lint": "next lint",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "cy": "cypress open",
    "ci:cy": "ELECTRON_RUN_AS_NODE=1 cypress run --headless --config-file cypress.config.ts --browser electron",
    "e2e": "start-server-and-test --expect 200 start http://127.0.0.1:3000 cy",
    "ci:e2e": "start-server-and-test --expect 200 build_and_start http://127.0.0.1:3000 ci:cy",
    "ci:test": "npm run lint && npm run ci:e2e",
    "test:unit": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@auth0/nextjs-auth0": "^2.2.1",
    "@aws-sdk/client-s3": "^3.433.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.16",
    "@legavee/redact": "*",
    "@lexical/react": "^0.12.2",
    "@next/font": "13.1.5",
    "@prisma/client": "^4.11.0",
    "@tailwindcss/forms": "^0.5.3",
    "add": "^2.0.6",
    "apollo-server-micro": "^3.12.1",
    "axios": "^1.3.4",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "framer-motion": "^10.0.1",
    "graphql": "^16.6.0",
    "http-error": "^0.0.6",
    "marked": "^4.2.12",
    "micro": "^10.0.1",
    "micro-cors": "^0.1.1",
    "next": "13.1.5",
    "openai": "^3.1.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-loading": "^2.0.3",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.8.1",
    "reactjs-popup": "^2.0.5",
    "turndown": "^7.1.2",

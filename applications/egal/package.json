{
  "name": "@legavee/egal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build_and_start": "npm run build && npm run start",
    "lint": "next lint",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "cy": "cypress open",
    "ci:cy": "ELECTRON_RUN_AS_NODE=1 cypress run --headless --config-file cypress.config.ts --browser electron",
    "e2e": "start-server-and-test --expect 200 start http://127.0.0.1:3000 cy",
    "ci:e2e": "start-server-and-test --expect 200 build_and_start http://127.0.0.1:3000 ci:cy",
    "ci:test": "npm run lint && npm run ci:e2e",
    "test:unit": "jest"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@next/font": "13.1.5",
    "@prisma/client": "^4.11.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "axios": "^1.3.4",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "framer-motion": "^10.0.1",
    "http-error": "^0.0.6",
    "next": "13.1.5",
    "openai": "^3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^6.8.1",
    "reactjs-popup": "^2.0.5",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/jest": "^29.4.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.4.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.11.0",
    "start-server-and-test": "^1.15.3",
    "tailwindcss": "^3.2.6",
    "ts-jest": "^29.0.5"
  }
}
